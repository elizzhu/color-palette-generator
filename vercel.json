{
    "version": 2,
    "name": "brand-generator",
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/photo",
            "dest": "/api/photo.js"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 